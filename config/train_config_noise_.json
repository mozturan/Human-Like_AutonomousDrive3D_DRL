{
    "Env": {
        "name": "donkey-generated-track-v0"
    },

    "Model" : "SAC-CL+Noise",

    "Agent": {
        "name": "SAC_",
        "ACTION_SIZE": 2,
        "HIDDEN_SIZE": 1024,
        "MIN_SIZE": 100,
        "BATCH_SIZE": 256,
        "MAX_ACTION": 0.8,
        "TEMPERATURE": 0.2,
        "TAU": 0.005,
        "GAMMA": 0.99,
        "USE_NOISE": true
    },

    "state_wrapper": {
        "name": "MultiGoalStateVector",
        "parameters": {
            "ae_path": "models/encoder_tracks/",
            "lidar_ae_path": "models/encoder_tracks/",
            "max_lidar_range": 20,
            "state_history": 2}
    },    

    "action_wrapper": {
        "name": "ActionClipping",
        "parameters": {}},

        "reward_wrapper": {
            "name": "MultiGoalReward",
            "parameters": {
                "max_cte": 4.0, 
                "max_delta": 1.6, 
                "sigma": 0.2, 
                "action_cost": 0.5, 
                "target_speed": 1.0    
            }
        }
    
}